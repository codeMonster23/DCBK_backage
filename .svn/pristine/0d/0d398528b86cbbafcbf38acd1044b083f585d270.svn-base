<script type="text/javascript">
    $('[data-toggle="popover"]').popover();

    $(".wsyj-icon2 img").click(function(){
        $(".pop1").show().insertAfter($(this));
    });
    $(".wsyj-icon3 img").click(function(){
        $(".pop2").show().insertAfter($(this));
    });
    $(".pop .btn-cancel").click(function(){
        $(this).parents(".pop").hide();
    });
    $(".navbar-right  a.user").click(function(){
      $(this).siblings(".login-dropdown").toggle();
    });
    $(".audit1 .detail table tr:odd").css("background-color","#fafafa");
    $(".audit1 .detail table tr").each(function(){
        $(this).find("td:first").addClass('chapter');
    });
    $(".audit1 .works-info table tr").each(function(){
        $(this).find("td:first").css("font-weight","bold");
    });
    $(".audit1 .audit-info table tr").each(function(){
        $(this).find("td:first").css("font-weight","bold");
    });
    $("table:last tr:even").css("background-color","#fafafa");
    var con_height=$(".content").outerHeight();
    $(".sidebar").height(con_height);
    
    $(".sidebar .nav-item").click(function() { 
        $(this).toggleClass('active').siblings().removeClass('active');
    }); 
    $(window).scroll(function(){
        var h1=$(window).scrollTop();
        if(h1>206){
            $(".tab-header").addClass('active');
            var $nav_a = $(".tab-nav a");
            var $content = $(".tab-content");

            //监听导航条被点击
            $nav_a.click(function(){
                var i = $(this).index();
                $("html,body").stop(true).animate({"scrollTop":$content.eq(i).offset().top - 40},1000);
            });
            //监听窗口的卷动
            $(window).scroll(function(){
                var A = $(window).scrollTop();  //已经卷动的高度
                if(A < $content.eq(1).offset().top - 40){
                    $nav_a.eq(0).addClass("active").siblings().removeClass("active");
                }else if(A < $content.eq(2).offset().top - 40){
                    $nav_a.eq(1).addClass("active").siblings().removeClass("active");

                }else if(A < $content.eq(3).offset().top - 40){
                    if(A==$("body").height()-$(window).height()){
                        $nav_a.eq(3).addClass("active").siblings().removeClass("active");
                    }else{
                        $nav_a.eq(2).addClass("active").siblings().removeClass("active");
                    }
                }
            });
        }else{
            $(".tab-header").removeClass('active');
        }
    });

    $(".checkbox-box .checkbox,.checkbox-label").click(function(){
      $(this).parent(".checkbox-box").toggleClass('active').siblings().removeClass('active');
    });
</script>
